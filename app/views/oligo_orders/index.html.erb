<h1>Listing oligo_orders</h1>

<table>
  <tr>
    <th> Oligo Name </th>
    <th> Gene Code  </th>
	<th> Enzyme Code </th>
	<th> Selector ID </th>
	<th> Amplicon Length </th>
  </tr>

<% for oligo_order in @oligo_orders %>
  <tr>
    <td><%=h oligo_order.oligo_name %></td>
    <td><%=h oligo_order.gene_code %></td>
	<td><%=h oligo_order.enzyme_code %></td>
	<td><%=h oligo_order.selector_nr %></td>
	<td><%=h oligo_order.selector.amplicon_length %></td>

    <td><%= link_to 'Show', oligo_order %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New oligo_order', new_oligo_order_path %>
