<table>
  <tr>
  	<th>Oligo Name</th>
    <th>Gene</th>
	<th>Enzyme</th>
	<th>Region</th>
	<th>Amplicon Start</th>
	<th>Amplicon Length</th>
  </tr>

<% for selector in @selectors %>
  <tr>
  	<td><%=h selector.oligo_name %></td>
    <td><%=h selector.gene_code %></td>
	<td><%=h selector.enzyme_code %></td>
	<td><%=h selector.region.roi_nr %></td>
	<td><%=h selector.amplicon_chr_start_pos %></td>
	<td><%=h selector.amplicon_length %></td>
	
    <td><%= link_to 'Show', selector %></td>
    <td><%= link_to 'Edit', edit_selector_path(selector) %></td>
    <td><%= link_to 'Delete', selector, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Back', selectorquery_url %>
