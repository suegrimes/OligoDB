<h1>Synthesis Orders</h1>
<% debug(@syntheses) %>

<table>
  <tr>
    <th>Researcher</th>
	<th>Order date</th>
	<th>Gene code</th>
  </tr>

<% for synthesis in @syntheses %>
  <tr>
    <td><%=h synthesis.researcher %></td>
	<td><%=h synthesis.created_at %></td>
	<td><%=h synthesis.gene_code %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New synthesis order', new_synthesis_path %>