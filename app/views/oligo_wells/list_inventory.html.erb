
<h3>Oligo Inventory
<% if @vol %>
     - Wells with less than <%= @vol %> ul
  <% end %>
</h3>

<table class="border">
  <tr>
	<th> Oligo name  </th>
    <th> Plate Number  </th>
	<th> Well Number </th>
	<th> Initial Volume </th>
	<th> Remaining Volume </th>
  </tr>

<% for well in @oligo_wells %>
	<tr>
	  <td><%=h well.oligo_name %></td>
	  <td><%=h well.oligo_plate_nr%></td>
	  <td><%=h well.oligo_well_nr%></td>
	  <td><%=h well.well_initial_volume%></td>
	  <td><%=h well.well_rem_volume%></td>
    </tr>
    <% end %>
</table>

<br />
<td><%= link_to 'Back', :back %></td>


