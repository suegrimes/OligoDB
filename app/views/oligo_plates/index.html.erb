<h1>Listing oligo_plates</h1>

<table>
	<tr>
		<th>Plate Nr</th>
		<th>Date Loaded</th>
    </tr>
  <% for plate in @oligo_plates %>
    <tr>
	    <td><%= link_to plate.oligo_plate_nr, :controller => "oligo_wells",
                       :action => "index", :plate_id => plate.id  %></td>
		<td><%= plate.created_at %></td>
    </tr>
  <% end %>
</table>




