
<h3>Oligo Inventory</h3>
<table class="border">
  <tr>
	<th> Oligo name  </th>
	<th> Flag </th>
    <th> Plate 1  </th>
	<th> Well </th>
	<th> Initial Vol </th>
	<th> Rem Vol </th>
	<th> Plate 2 </th>
	<th> Well </th>
	<th> Initial Vol </th>
	<th> Rem Vol </th>
	<th> Plate 3 </th>
	<th> Well </th>
	<th> Initial Vol </th>
	<th> Rem Vol </th>
  </tr>

<% for oligo in @oligo_designs %>
	<tr>
	  <td><%=h oligo.oligo_name %></td>
	  <td><%=h oligo.valid_oligo %></td>
	  <% for well in oligo.oligo_wells %>
	    <td><%=h well.oligo_plate_nr%> </td>
	    <td><%=h well.oligo_well_nr%></td>
	    <td><%=h well.well_initial_volume%></td>
	    <td><%=h well.well_rem_volume%></td>
    <% end %>
	</tr>
  <% end %>
</table>

<br />
<td><%= link_to 'Back', :back %></td>


